  },
  "dependencies": {
    "@blog/theme": "workspace:*",
    "vitepress-plugin-analyzer": "workspace:*",
    "vitepress-plugin-config": "workspace:*",
    "vitepress": "^1.5.0",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitepress-plugin-rss": "^0.3.0",
    "vitepress-sidebar": "^1.30.2"
  }
}