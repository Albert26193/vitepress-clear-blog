{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "allowJs": true,
    "types": [],
    "baseUrl": ".",
    "typeRoots": [
      "./node_modules/@types",
      "./.vitepress/theme"
    ],
    "paths": {
      "@/*": [
        "./.vitepress/*"
      ],
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".vitepress/**/*.ts",
    ".vitepress/**/*.d.ts",
    ".vitepress/**/*.tsx",
    ".vitepress/**/*.vue",
    "shims-vue.d.ts",
    "**/*.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}